{
  "projectId": "_",
  "orgId": "_",
  "settings": {
    "framework": "nextjs"
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors 'self';"
        }
      ]
    }
  ]
}
